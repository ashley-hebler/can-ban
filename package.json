{
  "name": "can-app",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "1.12.1",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "animated": "^0.2.2",
    "directory-tree": "^2.1.0",
    "fs": "^0.0.1-security",
    "mkdirp": "^0.5.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "prerender-node": "^3.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dfp": "^0.7.5",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.0",
    "react-router-dom": "^4.2.2",
    "react-snapshot": "^1.3.0",
    "react-transition-group": "^2.3.1",
    "svg-sprite": "^1.4.0",
    "webfontloader": "^1.6.28"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p parse-markdown watch-css start-js",
    "build-js": "react-scripts build && react-snapshot",
    "build": "npm-run-all parse-markdown build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && surge -p build",
    "parse-markdown": "node src/utils/parseMarkdown.js"
  }
}
